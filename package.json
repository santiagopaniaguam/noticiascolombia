{
  "name": "colombian-news-scraper",
  "version": "1.0.0",
  "description": "Web scraper for Colombian newspaper headlines with word frequency analysis",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
